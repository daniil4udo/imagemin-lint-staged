{
    "name": "@democrance/imagemin-lint-staged",
    "version": "2.0.0",
    "description": "Easily configure imagemin to work with lint-staged",
    "keywords": [
        "lint-staged",
        "imagemin"
    ],
    "license": "MIT",
    "author": "Daniil Chumachenko <daniil4udo@gmail.com>",
    "type": "module",
    "main": "./lib/index.mjs",
    "bin": {
        "image-lint-staged": "./bin/cli.mjs",
        "imagemin-lint-staged": "./bin/cli.mjs"
    },
    "scripts": {
        "lint": "eslint --ext .vue,.ts,.js,.mjs .",
        "test": "rm -rf tests/__snapshots__/index.spec.js.snap && vitest"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^0.31.0",
        "cosmiconfig": "^8.0.0",
        "defu": "^6.1.1",
        "eslint": "^8.28.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "imagemin": "^8.0.1",
        "pretty-bytes": "^6.0.0",
        "vite": "^3.2.4",
        "vitest": "^0.25.2"
    },
    "optionalDependencies": {
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-mozjpeg": "^10.0.0",
        "imagemin-optipng": "^8.0.0",
        "imagemin-svgo": "^10.0.1",
        "imagemin-webp": "^7.0.0"
    },
    "engines": {
        "node": ">=14"
    },
    "pnpm": {
        "overrides": {
            "svgo": "^3.0.0",
            "uuid": "^8.0.0"
        }
    }
