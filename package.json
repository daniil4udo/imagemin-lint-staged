{
    "name": "@democrance/imagemin-lint-staged",
    "version": "2.4.0",
    "description": "Easily configure imagemin to work with lint-staged",
    "keywords": [
        "lint-staged",
        "imagemin"
    ],
    "license": "MIT",
    "author": "Daniil Chumachenko <daniil4udo@gmail.com>",
    "type": "module",
    "bin": {
        "image-lint-staged": "./dist/bin/cli.js",
        "imagemin-lint-staged": "./dist/bin/cli.js"
    },
    "scripts": {
        "build": "rimraf dist && tsup",
        "lint": "eslint --fix --ext .ts,.js .",
        "prepublishOnly": "pnpm build",
        "release": "bumpp --commit --tag --no-push && pnpm publish --access public",
        "test": "rimraf tests/__snapshots__/index.spec.js.snap && vitest"
    },
    "dependencies": {
        "@democrance/utils": "^0.12.0",
        "cosmiconfig": "^8.2.0",
        "imagemin": "^8.0.1",
        "pretty-bytes": "^6.1.0"
    },
    "devDependencies": {
        "@democrance/eslint-config": "^0.8.0",
        "@tsconfig/node18": "^2.0.1",
        "@types/imagemin-gifsicle": "^7.0.1",
        "@types/imagemin-mozjpeg": "^8.0.1",
        "@types/imagemin-optipng": "^5.2.1",
        "@types/imagemin-svgo": "^10.0.2",
        "@types/imagemin-webp": "^7.0.0",
        "@types/node": "^18.16.19",
        "@vue/tsconfig": "^0.4.0",
        "bumpp": "^9.1.1",
        "eslint": "^8.44.0",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-mozjpeg": "^10.0.0",
        "imagemin-optipng": "^8.0.0",
        "imagemin-svgo": "^10.0.1",
        "imagemin-webp": "^8.0.0",
        "tsup": "^7.1.0",
        "typescript": "~5.0.3",
        "vitest": "^0.32.4"
    },
    "peerDependencies": {
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-mozjpeg": "^10.0.0",
        "imagemin-optipng": "^8.0.0",
        "imagemin-svgo": "^10.0.1",
        "imagemin-webp": "^8.0.0"
    },
    "peerDependenciesMeta": {
        "imagemin-gifsicle": {
            "optional": true
        },
        "imagemin-mozjpeg": {
            "optional": true
        },
        "imagemin-optipng": {
            "optional": true
        },
        "imagemin-svgo": {
            "optional": true
        },
        "imagemin-webp": {
            "optional": true
        }
    },
    "engines": {
        "node": ">=16"
    },
    "pnpm": {
        "overrides": {
            "rollup": "^3.0.0",
            "svgo": "^3.0.0",
            "uuid": "^8.0.0"
        }
    }
}
