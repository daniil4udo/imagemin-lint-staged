{
    "name": "@democrance/imagemin-lint-staged",
    "type": "module",
    "version": "3.0.2",
    "packageManager": "pnpm@10.0.0",
    "description": "Easily configure imagemin to work with lint-staged",
    "author": "Daniil Chumachenko <daniil4udo@gmail.com>",
    "license": "MIT",
    "keywords": [
        "lint-staged",
        "imagemin"
    ],
    "bin": {
        "image-lint-staged": "./dist/bin/cli.js",
        "imagemin-lint-staged": "./dist/bin/cli.js"
    },
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "build": "tsup",
        "lint": "eslint . --fix --color",
        "prepublishOnly": "pnpm build",
        "release": "bumpp --commit --tag --no-push && pnpm publish --access public",
        "test": "rm -rf tests/__snapshots__/index.spec.js.snap && vitest"
    },
    "dependencies": {
        "@democrance/utils": "^1.0.0",
        "cosmiconfig": "^9.0.0",
        "exif-reader": "^2.0.2",
        "pretty-bytes": "^6.1.1",
        "sharp": "^0.33.5",
        "svgo": "^3.3.2",
        "tinypool": "^1.0.2"
    },
    "devDependencies": {
        "@democrance/eslint-config": "^1.0.0",
        "@tsconfig/node22": "^22.0.0",
        "@types/node": "^22.13.1",
        "bumpp": "^10.0.2",
        "eslint": "^9.19.0",
        "tsup": "^8.3.6",
        "typescript": "~5.7.3",
        "vitest": "^3.0.5"
    }
}
